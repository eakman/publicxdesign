version: '3'
services:
{% for n in range(num_services) %}
  web{{ n }}:
    image: pxdlab:latest
    ports:
      - "{{ 8880 + n }}:8888"
    deploy:
      resources:
        limits:
          cpus: "{{ cpus }}"
          memory: "{{ memory }}"
        reservations:
          cpus: "{{ cpus }}"
          memory: "{{ memory }}"
{% endfor %}
